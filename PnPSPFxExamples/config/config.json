{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "pn-p-controls-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/pnPControls/PnPControlsWebPart.js",
          "manifest": "./src/webparts/pnPControls/PnPControlsWebPart.manifest.json"
        }
      ]
    },
    "pn-psp-curd-operations-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/pnPspCurdOperations/PnPspCurdOperationsWebPart.js",
          "manifest": "./src/webparts/pnPspCurdOperations/PnPspCurdOperationsWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "PnPControlsWebPartStrings": "lib/webparts/pnPControls/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "PnPspCurdOperationsWebPartStrings": "lib/webparts/pnPspCurdOperations/loc/{locale}.js"
  }
}
