{
    "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
    "version": "2.0",
    "bundles": {
        "jquerywith-react-web-part": {
            "components": [{
                "entrypoint": "./lib/webparts/jquerywithReact/JquerywithReactWebPart.js",
                "manifest": "./src/webparts/jquerywithReact/JquerywithReactWebPart.manifest.json"
            }]
        }
    },
    "externals": {
        "jquery": {
            "path": "node_modules/jquery/dist/jquery.min.js",
            "globalName": "jquery"
        },
        "boostrap": {
            "path": "node_modules/bootstrap/dist/js/bootstrap.min.js",
            "globalName": "bootstrap"
        },
        "tooltipser": {
            "path": "node_modules/tooltipser/dist/js/tooltipser.bundle.min.js",
            "globalName": "tooltipser"
        }
    },
    "localizedResources": {
        "JquerywithReactWebPartStrings": "lib/webparts/jquerywithReact/loc/{locale}.js"
    }
}