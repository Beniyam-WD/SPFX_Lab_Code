{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "sp-fx-bootstrap-styles-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/spFxBootstrapStyles/SpFxBootstrapStylesWebPart.js",
          "manifest": "./src/webparts/spFxBootstrapStyles/SpFxBootstrapStylesWebPart.manifest.json"
        }
      ]
    },
    "drop-down-menus-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/dropDownMenus/DropDownMenusWebPart.js",
          "manifest": "./src/webparts/dropDownMenus/DropDownMenusWebPart.manifest.json"
        }
      ]
    },
    "dynamic-control-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/dynamicControl/DynamicControlWebPart.js",
          "manifest": "./src/webparts/dynamicControl/DynamicControlWebPart.manifest.json"
        }
      ]
    },
    "dynamic-controlsfrom-s-plist-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/dynamicControlsfromSPlist/DynamicControlsfromSPlistWebPart.js",
          "manifest": "./src/webparts/dynamicControlsfromSPlist/DynamicControlsfromSPlistWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "SpFxBootstrapStylesWebPartStrings": "lib/webparts/spFxBootstrapStyles/loc/{locale}.js",
    "DropDownMenusWebPartStrings": "lib/webparts/dropDownMenus/loc/{locale}.js",
    "DynamicControlWebPartStrings": "lib/webparts/dynamicControl/loc/{locale}.js",
    "DynamicControlsfromSPlistWebPartStrings": "lib/webparts/dynamicControlsfromSPlist/loc/{locale}.js"
  }
}
